    "test:violations": "tsx src/lib/testing/run-violation-tests.ts",
    "test:heyapi": "tsx src/lib/testing/run-heyapi-tests.ts",
    "test": "jest"
  },
  "lint-staged": {
    "*.{ts,tsx}": ["biome check --write --no-errors-on-unmatched ./src"]
  },
